package principal;

public class Principal {

	public static void main(String[] args) {
		
		Pessoa p1, p2, p3;
		
		p1 = new Pessoa();
		System.out.println(p1.toString());
		
		p2 = new Pessoa("Joao", 26);
		System.out.println(p2.toString());
		
		p3 = new Pessoa("Pedro");
		System.out.println(p3.toString());

	}

}
